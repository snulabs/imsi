            @if (currentPage == "people")
    {
        <div class="people-page" style="min-height: 100vh; padding: 4rem 2rem; background: #f0f2f5;">
            <div class="container" style="max-width: 1400px; margin: 0 auto;">
                <header class="page-header" style="text-align: center; margin-bottom: 4rem;">
                    <h1 style="color: #2c3e50; font-size: 3rem;">People</h1>
                    <p class="subtitle" style="color: #7f8c8d; font-size: 1.2rem;">IMSI Lab Members</p>
                </header>

                @if (people == null)
                {
                    <div class="loading-container" style="text-align: center; padding: 4rem;">
                        <div class="loading-spinner" style="width: 50px; height: 50px; border: 4px solid #3498db; border-top: 4px solid transparent; border-radius: 50%; animation: spin 1s linear infinite; margin: 0 auto 1rem;"></div>
                        <p style="color: #7f8c8d;">Loading people from Google Sheets...</p>
                    </div>
                }
                else if (!people.Any())
                {
                    <div class="no-data" style="text-align: center; padding: 4rem; color: #7f8c8d;">
                        <h2>No people found.</h2>
                        <p>Check back later for updates.</p>
                    </div>
                }
                else
                {
                    @RenderPeopleGrid("Professors", professors, "#3498db")
                    @RenderPeopleGrid("Researchers", researchers, "#9b59b6")
                    @RenderPeopleGrid("Students", students, "#f39c12")

                    @if (otherCategories != null)
                    {
                        @foreach (var group in otherCategories)
                        {
                            @RenderPeopleGrid(group.Key, group.ToList(), GetCategoryColor(group.Key))
                        }
                    }
                }
            </div>
        </div>

        <style>
            @keyframes spin { 0% { transform: rotate(0deg); } 100% { transform: rotate(360deg); } }
            .people-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(350px, 1fr)); gap: 2rem; margin-top: 2rem; }
            .person-card { background: white; border-radius: 20px; box-shadow: 0 8px 30px rgba(0,0,0,0.1); padding: 2rem; transition: transform 0.3s ease; }
            .person-card:hover { transform: translateY(-5px); }
            .person-avatar { text-align: center; margin-bottom: 1.5rem; }
            .avatar-placeholder { width: 80px; height: 80px; border-radius: 50%; background: linear-gradient(135deg, #667eea 0%, #764ba2 100%); color: white; display: flex; align-items: center; justify-content: center; font-size: 2rem; font-weight: bold; margin: 0 auto; }
            .person-info h3 { color: #2c3e50; margin-bottom: 0.5rem; font-size: 1.5rem; }
            .person-affiliation { color: #7f8c8d; margin-bottom: 1rem; }
            .person-email { color: #3498db; margin-bottom: 1rem; }
            .person-tags { background: #ecf0f1; padding: 0.5rem 1rem; border-radius: 20px; font-size: 0.9rem; color: #2c3e50; }
            .person-details { margin-top: 1.5rem; border-top: 1px solid #ecf0f1; padding-top: 1.5rem; }
            .detail-item strong { color: #2c3e50; display: block; margin-bottom: 0.5rem; }
            .person-links { display: flex; gap: 1rem; justify-content: center; margin-top: 1.5rem; }
            .person-links a { display: flex; align-items: center; width: 40px; height: 40px; border-radius: 50%; background: #f8f9fa; transition: all 0.3s ease; }
            .person-links a:hover { background: #3498db; color: white; transform: scale(1.1); }
            .section-header { display: flex; justify-content: space-between; align-items: center; margin-bottom: 2rem; padding-bottom: 1rem; border-bottom: 3px solid; }
            .section-header h2 { margin: 0; color: #2c3e50; }
            .count { background: rgba(52,152,219,0.1); color: #3498db; padding: 0.5rem 1rem; border-radius: 20px; font-weight: 600; }
        </style>
    }















        


