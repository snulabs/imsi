@code {
    private List<HeroImage>? heroImages;
    private string currentPage = "home";

    private List<WebPerson>? people;
    private List<WebPerson>? professors;
    private List<WebPerson>? researchers;
    private List<WebPerson>? students;
    private IEnumerable<IGrouping<string, WebPerson>>? otherCategories;

    protected override async Task OnInitializedAsync()
    {
        heroImages = await Http.GetFromJsonAsync<List<HeroImage>>("sample-data/heroimages.json");
        
        // Google Sheets 데이터 로딩
        people = await SheetService.GetWebPeopleAsync();
        if (people != null)
        {
            professors = people.Where(p => p.Category == "Professor").ToList();
            researchers = people.Where(p => p.Category == "Researchers").ToList();
            students = people.Where(p => p.Category == "Student").ToList();
            
            otherCategories = people
                .Where(p => !string.IsNullOrWhiteSpace(p.Category) &&
                           p.Category != "Professor" && 
                           p.Category != "Student" && 
                           p.Category != "Researchers")
                .GroupBy(p => p.Category!)
                .OrderBy(g => g.Key);
        }
    }

    private void ChangeCurrentPage(string page)
    {
        currentPage = page;
        StateHasChanged();
    }

    // People 그리드 렌더링
    RenderFragment RenderPeopleGrid(string title, List<WebPerson>? list, string color) => 
    @<section>
        @if (list != null && list.Any())
        {
            <div class="section-header" style="border-bottom-color: @color;">
                <h2>@title (@list.Count @(list.Count == 1 ? "member" : "members"))</h2>
            </div>
            <div class="people-grid">
                @foreach (var person in list)
                {
                    @{
                        var firstName = person.Name?.Split(' ').FirstOrDefault();
                        var initial = !string.IsNullOrEmpty(firstName) ? firstName.Substring(0, 1) : "I";
                    }
                    <div class="person-card">
                        <div class="person-avatar">
                            <div class="avatar-placeholder">@initial</div>
                        </div>
                        <div class="person-info">
                            <h3>@person.Name</h3>
                            @if (!string.IsNullOrEmpty(person.Org) || !string.IsNullOrEmpty(person.Dept))
                            {
                                <p class="person-affiliation">
                                    <strong>@person.Org</strong>
                                    @if (!string.IsNullOrEmpty(person.Dept))
                                    {
                                        <span> – @person.Dept</span>
                                    }
                                    @if (!string.IsNullOrEmpty(person.SubOrg))
                                    {
                                        <span class="suborg"> (@person.SubOrg)</span>
                                    }
                                </p>
                            }
                            @if (!string.IsNullOrEmpty(person.Email))
                            {
                                <p class="person-email">
                                    📧 <a href="mailto:@person.Email">@person.Email</a>
                                </p>
                            }
                            @if (!string.IsNullOrEmpty(person.Tags))
                            {
                                <div class="person-tags">🏷️ @person.Tags</div>
                            }
                        </div>
                        @if (!string.IsNullOrEmpty(person.Education) || !string.IsNullOrEmpty(person.Experience))
                        {
                            <div class="person-details">
                                @if (!string.IsNullOrEmpty(person.Education))
                                {
                                    <div class="detail-item">
                                        <strong>Education</strong>
                                        <p>@((MarkupString)person.Education)</p>
                                    </div>
                                }
                                @if (!string.IsNullOrEmpty(person.Experience))
                                {
                                    <div class="detail-item">
                                        <strong>Experience</strong>
                                        <p>@((MarkupString)person.Experience)</p>
                                    </div>
                                }
                            </div>
                        }
                        <div class="person-links">
                            @if (!string.IsNullOrEmpty(person.Github))
                            {
                                <a href="@person.Github" target="_blank" title="GitHub">
                                    <svg viewBox="0 0 24 24" width="20" height="20" fill="currentColor">
                                        <path d="M12 .297c-6.63 0-12 5.373-12 12 0 5.303 3.438 9.8 8.205 11.385.6.113.82-.258.82-.577 0-.285-.01-1.04-.015-2.04-3.338.724-4.042-1.61-4.042-1.61C4.422 18.07 3.633 17.7 3.633 17.7c-1.087-.744.084-.729.084-.729 1.205.084 1.838 1.236 1.838 1.236 1.07 1.835 2.809 1.305 3.495.998.108-.776.417-1.305.76-1.605-2.665-.3-5.466-1.332-5.466-5.93 0-1.31.465-2.38 1.235-3.22-.135-.303-.54-1.523.105-3.176 0 0 1.005-.322 3.3 1.23.96-.267 1.98-.399 3-.405 1.02.006 2.04.138 3 .405 2.28-1.552 3.285-1.23 3.285-1.23.645 1.653.24 2.873.12 3.176.765.84 1.23 1.91 1.23 3.22 0 4.61-2.805 5.625-5.475 5.92.42.36.81 1.096.81 2.22 0 1.606-.015 2.896-.015 3.286 0 .315.21.69.825.57C20.565 22.092 24 17.592 24 12c0-6.627-5.373-12-12-12z"/>
                                    </svg>
                                </a>
                            }
                            @if (!string.IsNullOrEmpty(person.Blog))
                            {
                                <a href="@person.Blog" target="_blank" title="Blog">📝</a>
                            }
                            @if (!string.IsNullOrEmpty(person.Linkedin))
                            {
                                <a href="@person.Linkedin" target="_blank" title="LinkedIn">
                                    <svg viewBox="0 0 24 24" width="20" height="20" fill="currentColor">
                                        <path d="M20.447 20.452h-3.554v-5.569c0-1.328-.027-3.037-1.852-3.037-1.853 0-2.136 1.445-2.136 2.939v5.667H9.351V9h3.414v1.561h.046c.477-.9 1.637-1.85 3.37-1.85 3.601 0 4.267 2.37 4.267 5.455v6.286zM5.337 7.433a2.062 2.062 0 110-4.125 2.062 2.062 0 010 4.125zM6.779 20.452H3.895V9h2.884v11.452z"/>
                                    </svg>
                                </a>
                            }
                        </div>
                    </div>
                }
            </div>
        }
    </section>;

    string GetCategoryColor(string category) => category switch
    {
        "Team SUDAL" => "#f39c12",
        "Team PNI" => "#3498db",
        "Team fMRI" => "#9b59b6",
        "Team CEUS" => "#e74c3c",
        "Team HAEDAL" => "#9b59b6",
        _ => "#95a5a6"
    };
}


<style>
    #devil-effect {
        position: fixed;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        z-index: 9999;
        text-align: center;
        background: rgba(0, 0, 0, 0.8);
        color: #ff4444;
        padding: 2rem;
        border-radius: 15px;
        border: 3px solid #ff4444;
        font-family: 'Arial', sans-serif;
        font-weight: bold;
        font-size: 1.2rem;
        animation: devil-shake 2s ease-in-out;
        display: none;
    }
    
    .devil-char {
        font-size: 4rem;
        margin-bottom: 1rem;
        animation: devil-bounce 0.5s infinite alternate;
    }
    
    @@keyframes devil-shake {
        0%, 100% { transform: translate(-50%, -50%) rotate(0deg); }
        25% { transform: translate(-50%, -50%) rotate(-2deg); }
        75% { transform: translate(-50%, -50%) rotate(2deg); }
    }
    
    @@keyframes devil-bounce {
        0% { transform: translateY(0px); }
        100% { transform: translateY(-10px); }
    }

    .rank-icon {
        display: none;
    }
</style>

<div id="devil-effect">
    <div class="devil-char">😈</div>
    <div>YOU SHOULD HAVE DONE THIS WAY EARLIER!</div>
</div>

<script>
    window.seoyoungClickCount = 0;
    window.isDevilMode = false;
    window.sooyongClickCount = 0;
    window.isSkillSystemVisible = false;

    function initSeoyoungEasterEgg() {
        const seoyoungProfile = document.getElementById('seoyoung-profile');
        const seoyoungImg = document.getElementById('seoyoung-img');
        const devilEffect = document.getElementById('devil-effect');

        if (seoyoungProfile && seoyoungImg && devilEffect) {
            seoyoungProfile.onclick = function() {
                window.seoyoungClickCount++;
                console.log('Click count:', window.seoyoungClickCount, 'Devil mode:', window.isDevilMode);

                if (!window.isDevilMode && window.seoyoungClickCount === 3) {
                    // Show devil effect
                    devilEffect.style.display = 'block';

                    setTimeout(function() {
                        devilEffect.style.display = 'none';
                        seoyoungImg.src = 'images/profiles/4_1_profile_seoyoung_ju_devil.png';
                        window.isDevilMode = true;
                        window.seoyoungClickCount = 0;
                    }, 2000);
                }
                else if (window.isDevilMode && window.seoyoungClickCount === 3) {
                    // Show effect and revert to normal
                    devilEffect.style.display = 'block';

                    setTimeout(function() {
                        devilEffect.style.display = 'none';
                        seoyoungImg.src = 'images/profiles/4_profile_seoyoung_ju.png';
                        window.isDevilMode = false;
                        window.seoyoungClickCount = 0;
                    }, 2000);
                }
            };
        }
    }

    function initSooyongEasterEgg() {
        const sooyongProfile = document.getElementById('sooyong-profile');
        const skillLevelSystem = document.getElementById('skill-level-system');
        const rankIcons = document.querySelectorAll('.rank-icon');

        if (sooyongProfile && skillLevelSystem && rankIcons) {
            sooyongProfile.onclick = function() {
                window.sooyongClickCount++;
                console.log('Soo Yong Kim click count:', window.sooyongClickCount, 'Skill system visible:', window.isSkillSystemVisible);

                if (window.sooyongClickCount === 2) {
                    // Toggle skill level system and rank icons visibility
                    if (!window.isSkillSystemVisible) {
                        skillLevelSystem.style.display = 'block';
                        rankIcons.forEach(function(icon) {
                            icon.style.display = 'inline';
                        });
                        window.isSkillSystemVisible = true;
                        console.log('Skill Level System and rank icons revealed!');
                    } else {
                        skillLevelSystem.style.display = 'none';
                        rankIcons.forEach(function(icon) {
                            icon.style.display = 'none';
                        });
                        window.isSkillSystemVisible = false;
                        console.log('Skill Level System and rank icons hidden!');
                    }
                    window.sooyongClickCount = 0;
                }
            };
        }
    }

    function initEasterEggs() {
        initSeoyoungEasterEgg();
        initSooyongEasterEgg();
    }

    // Try to initialize multiple times to ensure it works
    if (document.readyState === 'loading') {
        document.addEventListener('DOMContentLoaded', initEasterEggs);
    } else {
        initEasterEggs();
    }

    // Also try after a short delay
    setTimeout(initEasterEggs, 1000);
</script>

