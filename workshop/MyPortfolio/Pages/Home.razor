</div>

<script>
    window.seoyoungClickCount = 0;
    window.isDevilMode = false;
    window.sooyongClickCount = 0;
    window.isSkillSystemVisible = false;

    function initSeoyoungEasterEgg() {
        const seoyoungProfile = document.getElementById('seoyoung-profile');
        const seoyoungImg = document.getElementById('seoyoung-img');
        const devilEffect = document.getElementById('devil-effect');

        if (seoyoungProfile && seoyoungImg && devilEffect) {
            seoyoungProfile.onclick = function() {
                window.seoyoungClickCount++;
                console.log('Click count:', window.seoyoungClickCount, 'Devil mode:', window.isDevilMode);

                if (!window.isDevilMode && window.seoyoungClickCount === 3) {
                    // Show devil effect
                    devilEffect.style.display = 'block';

                    setTimeout(function() {
                        devilEffect.style.display = 'none';
                        seoyoungImg.src = 'images/profiles/4_1_profile_seoyoung_ju_devil.png';
                        window.isDevilMode = true;
                        window.seoyoungClickCount = 0;
                    }, 2000);
                }
                else if (window.isDevilMode && window.seoyoungClickCount === 3) {
                    // Show effect and revert to normal
                    devilEffect.style.display = 'block';

                    setTimeout(function() {
                        devilEffect.style.display = 'none';
                        seoyoungImg.src = 'images/profiles/4_profile_seoyoung_ju.png';
                        window.isDevilMode = false;
                        window.seoyoungClickCount = 0;
                    }, 2000);
                }
            };
        }
    }

    function initSooyongEasterEgg() {
        const sooyongProfile = document.getElementById('sooyong-profile');
        const skillLevelSystem = document.getElementById('skill-level-system');
        const rankIcons = document.querySelectorAll('.rank-icon');

        if (sooyongProfile && skillLevelSystem && rankIcons) {
            sooyongProfile.onclick = function() {
                window.sooyongClickCount++;
                console.log('Soo Yong Kim click count:', window.sooyongClickCount, 'Skill system visible:', window.isSkillSystemVisible);

                if (window.sooyongClickCount === 2) {
                    // Toggle skill level system and rank icons visibility
                    if (!window.isSkillSystemVisible) {
                        skillLevelSystem.style.display = 'block';
                        rankIcons.forEach(function(icon) {
                            icon.style.display = 'inline';
                        });
                        window.isSkillSystemVisible = true;
                        console.log('Skill Level System and rank icons revealed!');
                    } else {
                        skillLevelSystem.style.display = 'none';
                        rankIcons.forEach(function(icon) {
                            icon.style.display = 'none';
                        });
                        window.isSkillSystemVisible = false;
                        console.log('Skill Level System and rank icons hidden!');
                    }
                    window.sooyongClickCount = 0;
                }
            };
        }
    }

    function initEasterEggs() {
        initSeoyoungEasterEgg();
        initSooyongEasterEgg();
    }

    // Try to initialize multiple times to ensure it works
    if (document.readyState === 'loading') {
        document.addEventListener('DOMContentLoaded', initEasterEggs);
    } else {
        initEasterEggs();
    }

    // Also try after a short delay
    setTimeout(initEasterEggs, 1000);
</script>

